    %load/vec4 v0x608396ba2fb0_0;
    %cmp/u;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x608396bb7f30_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x608396ba2fb0_0;
    %load/vec4 v0x608396bb7d80_0;
    %cmp/e;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x608396bb7e60_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x608396ba2fb0_0;
    %load/vec4 v0x608396bb7d80_0;
    %cmp/u;
    %jmp/0xz  T_0.4, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x608396bb7ff0_0, 0, 1;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x608396ba2c40;
T_1 ;
    %vpi_call 2 10 "$monitor", "time=%0t|A=%b|B=%b|greater_than=%b|equal=%b|lesser_than=%b", $time, v0x608396bb81a0_0, v0x608396bb8280_0, v0x608396bb8450_0, v0x608396bb8350_0, v0x608396bb86b0_0 {0 0 0};
