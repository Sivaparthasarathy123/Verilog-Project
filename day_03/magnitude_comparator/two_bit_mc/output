    %cmpi/s 4, 0, 32;
    %jmp/0xz T_1.3, 5;
    %load/vec4 v0x608396bb8520_0;
    %pad/s 2;
    %store/vec4 v0x608396bb81a0_0, 0, 2;
    %load/vec4 v0x608396bb8610_0;
    %pad/s 2;
    %store/vec4 v0x608396bb8280_0, 0, 2;
    %delay 5, 0;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x608396bb8610_0;
