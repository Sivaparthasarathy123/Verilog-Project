    .port_info 0 /INPUT 4 "A";
    .port_info 1 /INPUT 4 "B";
    .port_info 2 /OUTPUT 1 "greater_than";
    .port_info 3 /OUTPUT 1 "equal";
    .port_info 4 /OUTPUT 1 "less_than";
