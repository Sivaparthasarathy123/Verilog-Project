v0x600a9e6e6d50_0 .var "greater_than", 0 0;
v0x600a9e6e6e10_0 .var "less_than", 0 0;
E_0x600a9e697a70 .event anyedge, v0x600a9e6d1fb0_0, v0x600a9e6e6ba0_0;
    .scope S_0x600a9e6d1dd0;
T_0 ;
    %wait E_0x600a9e697a70;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600a9e6e6d50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600a9e6e6c80_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600a9e6e6e10_0, 0, 1;
    %load/vec4 v0x600a9e6e6ba0_0;
    %load/vec4 v0x600a9e6d1fb0_0;
    %cmp/u;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x600a9e6e6d50_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x600a9e6d1fb0_0;
    %load/vec4 v0x600a9e6e6ba0_0;
    %cmp/e;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x600a9e6e6c80_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x600a9e6d1fb0_0;
    %load/vec4 v0x600a9e6e6ba0_0;
    %cmp/u;
    %jmp/0xz  T_0.4, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x600a9e6e6e10_0, 0, 1;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x600a9e6d1c40;
T_1 ;
    %vpi_call 2 10 "$monitor", "time=%0t|A=%b|B=%b|greater_than=%b|equal=%b|less_than=%b", $time, v0x600a9e6e6fc0_0, v0x600a9e6e70a0_0, v0x600a9e6e7270_0, v0x600a9e6e7170_0, v0x600a9e6e74d0_0 {0 0 0};
    %vpi_call 2 11 "$dumpfile", "four_bit_mc.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x600a9e6e7340_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x600a9e6e7340_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_1.1, 5;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x600a9e6e7430_0, 0, 32;
T_1.2 ;
    %load/vec4 v0x600a9e6e7430_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_1.3, 5;
    %load/vec4 v0x600a9e6e7340_0;
    %pad/s 4;
    %store/vec4 v0x600a9e6e6fc0_0, 0, 4;
    %load/vec4 v0x600a9e6e7430_0;
    %pad/s 4;
    %store/vec4 v0x600a9e6e70a0_0, 0, 4;
    %delay 5, 0;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x600a9e6e7430_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x600a9e6e7430_0, 0, 32;
    %jmp T_1.2;
T_1.3 ;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x600a9e6e7340_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x600a9e6e7340_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
