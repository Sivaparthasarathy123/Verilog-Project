    %load/vec4 v0x61a9c884c7a0_0;
    %cmp/u;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61a9c884c930_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x61a9c8838b80_0;
    %load/vec4 v0x61a9c884c7a0_0;
    %cmp/e;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61a9c884c860_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
