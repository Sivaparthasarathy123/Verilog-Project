v0x61a9c884c9f0_0 .var "less_than", 0 0;
E_0x61a9c87feb40 .event anyedge, v0x61a9c8838b80_0, v0x61a9c884c7a0_0;
    .scope S_0x61a9c8838980;
T_0 ;
    %wait E_0x61a9c87feb40;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61a9c884c930_0, 0, 1;
