    %load/vec4 v0x61a9c8838b80_0;
    %cmp/u;
    %jmp/0xz  T_0.4, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61a9c884c9f0_0, 0, 1;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x61a9c88387f0;
T_1 ;
    %vpi_call 2 9 "$monitor", "time=%0t|a=%b|b=%b|greater_than=%b|equalto=%b|less_than=%b", $time, v0x61a9c884cba0_0, v0x61a9c884cc60_0, v0x61a9c884ce30_0, v0x61a9c884cd30_0, v0x61a9c884cf00_0 {0 0 0};
