#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c66364c7cf0 .scope module, "p_encoder_4x2_" "p_encoder_4x2_" 2 1;
 .timescale 0 0;
v0x5c66364dc300_0 .var "a", 3 0;
v0x5c66364dc3f0_0 .var "en", 0 0;
v0x5c66364dc4c0_0 .net "y", 1 0, v0x5c66364dc1c0_0;  1 drivers
S_0x5c66364c7e80 .scope module, "inst" "p_encoder_4x2" 2 5, 3 1 0, S_0x5c66364c7cf0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /INPUT 1 "en";
    .port_info 2 /OUTPUT 2 "y";
v0x5c66364c80d0_0 .net "a", 3 0, v0x5c66364dc300_0;  1 drivers
v0x5c66364dc100_0 .net "en", 0 0, v0x5c66364dc3f0_0;  1 drivers
v0x5c66364dc1c0_0 .var "y", 1 0;
E_0x5c663648e050 .event anyedge, v0x5c66364c80d0_0, v0x5c66364dc100_0;
    .scope S_0x5c66364c7e80;
T_0 ;
    %wait E_0x5c663648e050;
    %load/vec4 v0x5c66364dc100_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x5c66364c80d0_0;
    %parti/s 1, 3, 3;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x5c66364dc1c0_0, 0, 2;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5c66364c80d0_0;
    %parti/s 1, 2, 3;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.4, 4;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5c66364dc1c0_0, 0, 2;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x5c66364c80d0_0;
    %parti/s 1, 1, 2;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.6, 4;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5c66364dc1c0_0, 0, 2;
    %jmp T_0.7;
T_0.6 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5c66364dc1c0_0, 0, 2;
T_0.7 ;
T_0.5 ;
T_0.3 ;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 3, 2;
    %store/vec4 v0x5c66364dc1c0_0, 0, 2;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5c66364c7cf0;
T_1 ;
    %vpi_call 2 7 "$monitor", "time=%0t| a=%b| en=%b| y=%b", $time, v0x5c66364dc300_0, v0x5c66364dc3f0_0, v0x5c66364dc4c0_0 {0 0 0};
    %vpi_call 2 8 "$dumpfile", "p_encoder.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c66364dc3f0_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5c66364dc300_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c66364dc3f0_0, 0, 1;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x5c66364dc300_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c66364dc3f0_0, 0, 1;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x5c66364dc300_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c66364dc3f0_0, 0, 1;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x5c66364dc300_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c66364dc3f0_0, 0, 1;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x5c66364dc300_0, 0, 4;
    %delay 5, 0;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "p_encoder_4x2_tb.v";
