#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5b0f239f90e0 .scope module, "parity_checker_tb" "parity_checker_tb" 2 1;
 .timescale 0 0;
v0x5b0f23a0dc80_0 .var "a", 15 0;
v0x5b0f23a0dd40_0 .net "even_parity", 0 0, v0x5b0f23a0d8b0_0;  1 drivers
v0x5b0f23a0de10_0 .var/i "i", 31 0;
v0x5b0f23a0dee0_0 .net "odd_parity", 0 0, v0x5b0f23a0da30_0;  1 drivers
S_0x5b0f239f9270 .scope module, "inst" "parity_checker" 2 6, 3 1 0, S_0x5b0f239f90e0;
 .timescale 0 0;
    .port_info 0 /INPUT 16 "a";
    .port_info 1 /OUTPUT 1 "even_parity";
    .port_info 2 /OUTPUT 1 "odd_parity";
v0x5b0f239be490_0 .net "a", 15 0, v0x5b0f23a0dc80_0;  1 drivers
v0x5b0f23a0d8b0_0 .var "even_parity", 0 0;
v0x5b0f23a0d970_0 .var/i "i", 31 0;
v0x5b0f23a0da30_0 .var "odd_parity", 0 0;
v0x5b0f23a0daf0_0 .var "parity_value", 0 0;
E_0x5b0f239bea70 .event anyedge, v0x5b0f23a0daf0_0, v0x5b0f239be490_0;
    .scope S_0x5b0f239f9270;
T_0 ;
    %wait E_0x5b0f239bea70;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b0f23a0daf0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5b0f23a0d970_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5b0f23a0d970_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5b0f23a0daf0_0;
    %load/vec4 v0x5b0f239be490_0;
    %load/vec4 v0x5b0f23a0d970_0;
    %part/s 1;
    %xor;
    %store/vec4 v0x5b0f23a0daf0_0, 0, 1;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5b0f23a0d970_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5b0f23a0d970_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x5b0f23a0daf0_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b0f23a0d8b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b0f23a0da30_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b0f23a0d8b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b0f23a0da30_0, 0, 1;
T_0.3 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5b0f239f90e0;
T_1 ;
    %vpi_call 2 8 "$monitor", "time=%0t| a=%b| even_parity=%b| odd_parity=%b", $time, v0x5b0f23a0dc80_0, v0x5b0f23a0dd40_0, v0x5b0f23a0dee0_0 {0 0 0};
    %vpi_call 2 9 "$dumpfile", "parity_checker.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5b0f23a0de10_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x5b0f23a0de10_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_1.1, 5;
    %vpi_func 2 12 "$random" 32 {0 0 0};
    %pad/s 16;
    %store/vec4 v0x5b0f23a0dc80_0, 0, 16;
    %vpi_func 2 13 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000000001 {0 0 0};
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x5b0f23a0dc80_0;
    %inv;
    %store/vec4 v0x5b0f23a0dc80_0, 0, 16;
    %vpi_call 2 15 "$display", "value flipped %0d!", v0x5b0f23a0de10_0 {0 0 0};
    %delay 10, 0;
T_1.2 ;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5b0f23a0de10_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5b0f23a0de10_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "parity_checker_tb.v";
    "parity_checker.v";
