#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6057696b8180 .scope module, "mux_2x1_tb" "mux_2x1_tb" 2 1;
 .timescale 0 0;
v0x6057696cb0a0_0 .var "a", 0 0;
v0x6057696cb160_0 .var "b", 0 0;
v0x6057696cb230_0 .var "sel", 0 0;
v0x6057696cb330_0 .net "y", 0 0, L_0x6057696cb6f0;  1 drivers
S_0x6057696b8310 .scope module, "inst" "mux_2x1" 2 5, 3 1 0, S_0x6057696b8180;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /INPUT 1 "sel";
    .port_info 3 /OUTPUT 1 "y";
L_0x6057696cb400 .functor AND 1, v0x6057696cb0a0_0, L_0x6057696cb630, C4<1>, C4<1>;
L_0x6057696cb4f0 .functor AND 1, v0x6057696cb160_0, v0x6057696cb230_0, C4<1>, C4<1>;
L_0x6057696cb630 .functor NOT 1, v0x6057696cb230_0, C4<0>, C4<0>, C4<0>;
L_0x6057696cb6f0 .functor OR 1, L_0x6057696cb400, L_0x6057696cb4f0, C4<0>, C4<0>;
v0x6057696b8990_0 .net "a", 0 0, v0x6057696cb0a0_0;  1 drivers
v0x6057696cab40_0 .net "b", 0 0, v0x6057696cb160_0;  1 drivers
v0x6057696cac00_0 .net "sel", 0 0, v0x6057696cb230_0;  1 drivers
v0x6057696cacd0_0 .net "w1", 0 0, L_0x6057696cb630;  1 drivers
v0x6057696cad90_0 .net "w2", 0 0, L_0x6057696cb400;  1 drivers
v0x6057696caea0_0 .net "w3", 0 0, L_0x6057696cb4f0;  1 drivers
v0x6057696caf60_0 .net "y", 0 0, L_0x6057696cb6f0;  alias, 1 drivers
    .scope S_0x6057696b8180;
T_0 ;
    %vpi_call 2 8 "$monitor", "time=%0t |a=%b |b=%b |sel=%b |y=%b", $time, v0x6057696cb0a0_0, v0x6057696cb160_0, v0x6057696cb230_0, v0x6057696cb330_0 {0 0 0};
    %vpi_call 2 9 "$dumpfile", "muxtest.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x6057696b8180;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6057696cb0a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6057696cb160_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6057696cb230_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6057696cb0a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6057696cb160_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6057696cb230_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "mux_2x1_tb.v";
