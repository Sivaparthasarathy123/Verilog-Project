:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5979acb6a2f0 .scope module, "or_gate_tb" "or_gate_tb" 2 1;
 .timescale 0 0;
v0x5979acb7bdf0_0 .var "a", 0 0;
v0x5979acb7bec0_0 .var "b", 0 0;
v0x5979acb7bf90_0 .net "y", 0 0, L_0x5979acb7c090;  1 drivers
S_0x5979acb6a480 .scope module, "inst" "or_gate" 2 4, 3 1 0, S_0x5979acb6a2f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x5979acb7c090 .functor OR 1, v0x5979acb7bdf0_0, v0x5979acb7bec0_0, C4<0>, C4<0>;
v0x5979acb31490_0 .net "a", 0 0, v0x5979acb7bdf0_0;  1 drivers
v0x5979acb7bc10_0 .net "b", 0 0, v0x5979acb7bec0_0;  1 drivers
v0x5979acb7bcd0_0 .net "y", 0 0, L_0x5979acb7c090;  alias, 1 drivers
    .scope S_0x5979acb6a2f0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5979acb7bdf0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5979acb7bec0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5979acb7bdf0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5979acb7bec0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5979acb7bdf0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5979acb7bec0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5979acb7bdf0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5979acb7bec0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x5979acb6a2f0;
T_1 ;
    %vpi_call 2 14 "$monitor", " a=%b| b=%b| y=%b", v0x5979acb7bdf0_0, v0x5979acb7bec0_0, v0x5979acb7bf90_0 {0 0 0};
    %vpi_call 2 15 "$dumpfile", "or_gate_tb.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
