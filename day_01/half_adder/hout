    %end;
    .thread T_0;
    .scope S_0x58588fa737b0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58588fa870d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58588fa87190_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58588fa870d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58588fa87190_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58588fa870d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58588fa87190_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58588fa870d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
