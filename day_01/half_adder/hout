    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "sum";
    .port_info 3 /OUTPUT 1 "carry";
L_0x58588fa87430 .functor XOR 1, v0x58588fa870d0_0, v0x58588fa87190_0, C4<0>, C4<0>;
L_0x58588fa87590 .functor AND 1, v0x58588fa870d0_0, v0x58588fa87190_0, C4<1>, C4<1>;
v0x58588fa73b90_0 .net "a", 0 0, v0x58588fa870d0_0;  1 drivers
v0x58588fa86e30_0 .net "b", 0 0, v0x58588fa87190_0;  1 drivers
