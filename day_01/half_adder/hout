v0x58588fa86f90_0 .net "sum", 0 0, L_0x58588fa87430;  alias, 1 drivers
    .scope S_0x58588fa737b0;
T_0 ;
    %vpi_call 2 8 "$monitor", "time=%0t |a=%b |b=%b|sum=%b|carry=%b", $time, v0x58588fa870d0_0, v0x58588fa87190_0, v0x58588fa87360_0, v0x58588fa87260_0 {0 0 0};
