    .scope S_0x5f787e513490;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x5f787e565de0_0;
    %inv;
    %store/vec4 v0x5f787e565de0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5f787e513490;
T_4 ;
    %vpi_call 2 13 "$monitor", "%0t|clk=%b|reset=%b|bx=%b|q=%b", $time, v0x5f787e565de0_0, v0x5f787e565fb0_0, v0x5f787e5660a0_0, v0x5f787e565ef0_0 {0 0 0};
