E_0x5f787e54fa10 .event anyedge, v0x5f787e5655d0_0, v0x5f787e565cf0_0;
S_0x5f787e550510 .scope module, "inst" "DFF" 3 57, 3 83 0, S_0x5f787e513620;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "D";
    .port_info 3 /OUTPUT 1 "Q";
v0x5f787e550780_0 .net "D", 0 0, v0x5f787e565990_0;  1 drivers
v0x5f787e5655d0_0 .var "Q", 0 0;
v0x5f787e565690_0 .net "clk", 0 0, v0x5f787e565de0_0;  alias, 1 drivers
v0x5f787e565760_0 .net "reset", 0 0, v0x5f787e565fb0_0;  alias, 1 drivers
E_0x5f787e54f260 .event posedge, v0x5f787e565760_0, v0x5f787e565690_0;
    .scope S_0x5f787e550510;
T_0 ;
    %wait E_0x5f787e54f260;
    %load/vec4 v0x5f787e565760_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5f787e5655d0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5f787e550780_0;
    %assign/vec4 v0x5f787e5655d0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5f787e513620;
T_1 ;
    %wait E_0x5f787e54fa10;
    %load/vec4 v0x5f787e565c00_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %jmp T_1.2;
T_1.0 ;
    %load/vec4 v0x5f787e565cf0_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_1.3, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f787e565990_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f787e565a60_0, 0, 1;
    %jmp T_1.4;
T_1.3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f787e565990_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f787e565a60_0, 0, 1;
T_1.4 ;
    %jmp T_1.2;
T_1.1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f787e565990_0, 0, 1;
    %load/vec4 v0x5f787e565cf0_0;
    %inv;
    %store/vec4 v0x5f787e565a60_0, 0, 1;
    %jmp T_1.2;
T_1.2 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5f787e513490;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f787e565de0_0, 0, 1;
    %end;
