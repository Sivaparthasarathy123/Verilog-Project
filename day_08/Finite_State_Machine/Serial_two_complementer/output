    %store/vec4 v0x5f787e565fb0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f787e5660a0_0, 0, 1;
    %delay 10000, 0;
