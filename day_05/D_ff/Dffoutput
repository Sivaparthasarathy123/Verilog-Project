#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5bf45f8e65c0 .scope module, "D_ff_tb" "D_ff_tb" 2 1;
 .timescale 0 0;
v0x5bf45f8f8c60_0 .var "D", 0 0;
v0x5bf45f8f8d30_0 .net "Q", 0 0, v0x5bf45f8f8a80_0;  1 drivers
v0x5bf45f8f8e00_0 .var "clk", 0 0;
S_0x5bf45f8e6750 .scope module, "inst" "D_ff" 2 6, 3 1 0, S_0x5bf45f8e65c0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "D";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /OUTPUT 1 "Q";
v0x5bf45f8ad490_0 .net "D", 0 0, v0x5bf45f8f8c60_0;  1 drivers
v0x5bf45f8f8a80_0 .var "Q", 0 0;
v0x5bf45f8f8b40_0 .net "clk", 0 0, v0x5bf45f8f8e00_0;  1 drivers
E_0x5bf45f8adb00 .event posedge, v0x5bf45f8f8b40_0;
    .scope S_0x5bf45f8e6750;
T_0 ;
    %wait E_0x5bf45f8adb00;
    %load/vec4 v0x5bf45f8ad490_0;
    %assign/vec4 v0x5bf45f8f8a80_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5bf45f8e65c0;
T_1 ;
    %vpi_call 2 9 "$monitor", "%0t|D=%b|clk=%b|Q=%b", $time, v0x5bf45f8f8c60_0, v0x5bf45f8f8e00_0, v0x5bf45f8f8d30_0 {0 0 0};
    %vpi_call 2 10 "$dumpfile", "d_ff.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5bf45f8f8e00_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x5bf45f8f8e00_0;
    %inv;
    %store/vec4 v0x5bf45f8f8e00_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5bf45f8e65c0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5bf45f8f8c60_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5bf45f8f8c60_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5bf45f8f8c60_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5bf45f8f8c60_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "D_ff_tb.v";
    "D_ff.v";
