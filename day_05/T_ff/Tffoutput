#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5f4c8e616d70 .scope module, "T_ff_tb" "T_ff_tb" 2 1;
 .timescale 0 0;
v0x5f4c8e629e40_0 .net "Q", 0 0, v0x5f4c8e5dd490_0;  1 drivers
v0x5f4c8e629f10_0 .var "T", 0 0;
v0x5f4c8e629fe0_0 .var "clk", 0 0;
S_0x5f4c8e616f00 .scope module, "inst" "T_ff" 2 6, 3 1 0, S_0x5f4c8e616d70;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "T";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /OUTPUT 1 "Q";
v0x5f4c8e5dd490_0 .var "Q", 0 0;
v0x5f4c8e629c60_0 .net "T", 0 0, v0x5f4c8e629f10_0;  1 drivers
v0x5f4c8e629d20_0 .net "clk", 0 0, v0x5f4c8e629fe0_0;  1 drivers
E_0x5f4c8e5ddb00 .event posedge, v0x5f4c8e629d20_0;
    .scope S_0x5f4c8e616f00;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4c8e5dd490_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x5f4c8e616f00;
T_1 ;
    %wait E_0x5f4c8e5ddb00;
    %load/vec4 v0x5f4c8e629c60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x5f4c8e5dd490_0;
    %inv;
    %assign/vec4 v0x5f4c8e5dd490_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5f4c8e5dd490_0;
    %assign/vec4 v0x5f4c8e5dd490_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5f4c8e616d70;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4c8e629fe0_0, 0, 1;
T_2.0 ;
    %delay 5, 0;
    %load/vec4 v0x5f4c8e629fe0_0;
    %inv;
    %store/vec4 v0x5f4c8e629fe0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5f4c8e616d70;
T_3 ;
    %vpi_call 2 14 "$monitor", "%0t|T=%b|clk=%b|Q=%b", $time, v0x5f4c8e629f10_0, v0x5f4c8e629fe0_0, v0x5f4c8e629e40_0 {0 0 0};
    %vpi_call 2 15 "$dumpfile", "T_ff.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4c8e629f10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f4c8e629f10_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4c8e629f10_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f4c8e629f10_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4c8e629f10_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f4c8e629f10_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "T_ff_tb.v";
    "T_ff.v";
