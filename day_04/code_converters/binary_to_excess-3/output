    .port_info 0 /INPUT 4 "b";
    .port_info 1 /OUTPUT 4 "x";
v0x613284515a90_0 .net "b", 3 0, v0x6132845277e0_0;  1 drivers
v0x6132845276a0_0 .var "x", 3 0;
E_0x6132844dca30 .event anyedge, v0x613284515a90_0;
    .scope S_0x613284515890;
T_0 ;
    %wait E_0x6132844dca30;
    %load/vec4 v0x613284515a90_0;
    %addi 3, 0, 4;
    %store/vec4 v0x6132845276a0_0, 0, 4;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x613284515700;
T_1 ;
    %vpi_call 2 7 "$monitor", "time=%0t|b=%b|x=%b", $time, v0x6132845277e0_0, v0x613284527960_0 {0 0 0};
    %vpi_call 2 8 "$dumpfile", "binary_to_excess-3.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x6132845277e0_0, 0, 4;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6132845278a0_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x6132845278a0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x6132845278a0_0;
    %pad/s 4;
    %store/vec4 v0x6132845277e0_0, 0, 4;
    %delay 5, 0;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x6132845278a0_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x6132845278a0_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
