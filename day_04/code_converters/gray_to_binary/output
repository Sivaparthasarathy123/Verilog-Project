#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c7d861e2580 .scope module, "gray_to_binary_tb" "gray_to_binary_tb" 2 1;
 .timescale 0 0;
v0x5c7d861f56a0_0 .net "b", 3 0, v0x5c7d861e28f0_0;  1 drivers
v0x5c7d861f5760_0 .var "g", 3 0;
v0x5c7d861f5830_0 .var/i "i", 31 0;
S_0x5c7d861e2710 .scope module, "inst" "gray_to_binary" 2 5, 3 1 0, S_0x5c7d861e2580;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "g";
    .port_info 1 /OUTPUT 4 "b";
v0x5c7d861e28f0_0 .var "b", 3 0;
v0x5c7d861f5560_0 .net "g", 3 0, v0x5c7d861f5760_0;  1 drivers
E_0x5c7d861a8ee0 .event anyedge, v0x5c7d861f5560_0;
    .scope S_0x5c7d861e2710;
T_0 ;
    %wait E_0x5c7d861a8ee0;
    %load/vec4 v0x5c7d861f5560_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7d861e28f0_0, 4, 1;
    %load/vec4 v0x5c7d861f5560_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x5c7d861f5560_0;
    %parti/s 1, 2, 3;
    %xor;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7d861e28f0_0, 4, 1;
    %load/vec4 v0x5c7d861f5560_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x5c7d861f5560_0;
    %parti/s 1, 1, 2;
    %xor;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7d861e28f0_0, 4, 1;
    %load/vec4 v0x5c7d861f5560_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x5c7d861f5560_0;
    %parti/s 1, 0, 2;
    %xor;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c7d861e28f0_0, 4, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5c7d861e2580;
T_1 ;
    %vpi_call 2 7 "$monitor", "time=%0t|g=%b|b=%b", $time, v0x5c7d861f5760_0, v0x5c7d861f56a0_0 {0 0 0};
    %vpi_call 2 8 "$dumpfile", "gray_to_binary.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5c7d861f5760_0, 0, 4;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5c7d861f5830_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x5c7d861f5830_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x5c7d861f5830_0;
    %pad/s 4;
    %store/vec4 v0x5c7d861f5760_0, 0, 4;
    %delay 5, 0;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5c7d861f5830_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5c7d861f5830_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "gray_to_binary_tb.v";
